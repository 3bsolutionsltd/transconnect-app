# TransConnect Web Portal - Production Environment
# API Configuration
NEXT_PUBLIC_API_URL=https://transconnect-app-44ie.onrender.com/api
NEXT_PUBLIC_API_VERSION=v1
NEXT_PUBLIC_ENVIRONMENT=production

# Features
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# Maps & Location
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Payment Integration
NEXT_PUBLIC_PAYMENT_ENVIRONMENT=sandbox
NEXT_PUBLIC_ENABLE_MTN_MOMO=true
NEXT_PUBLIC_ENABLE_AIRTEL_MONEY=true
NEXT_PUBLIC_ENABLE_FLUTTERWAVE=true

# Security
NEXT_PUBLIC_SECURE_COOKIES=true
NEXT_PUBLIC_ENABLE_CSP=true

# Performance
NEXT_PUBLIC_ENABLE_COMPRESSION=true
NEXT_PUBLIC_CACHE_TTL=300

# Contact Information
NEXT_PUBLIC_SUPPORT_EMAIL=support@transconnect.ug
NEXT_PUBLIC_SUPPORT_PHONE=+256700000000