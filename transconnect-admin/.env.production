# TransConnect Admin Dashboard - Production Environment
# API Configuration
REACT_APP_API_URL=https://transconnect-app-44ie.onrender.com
REACT_APP_API_VERSION=v1
REACT_APP_ENVIRONMENT=production

# Features
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_REALTIME=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Admin Features
REACT_APP_ENABLE_BULK_OPERATIONS=true
REACT_APP_ENABLE_ADVANCED_REPORTS=true
REACT_APP_ENABLE_SYSTEM_MONITOR=true

# Security
REACT_APP_SESSION_TIMEOUT=3600
REACT_APP_ENABLE_2FA=true
REACT_APP_SECURE_COOKIES=true

# Performance
REACT_APP_ENABLE_CACHING=true
REACT_APP_CACHE_TTL=300
REACT_APP_PAGINATION_SIZE=20

# Monitoring
REACT_APP_ERROR_REPORTING=true
REACT_APP_PERFORMANCE_MONITORING=true

# Contact Information
REACT_APP_SUPPORT_EMAIL=admin@transconnect.ug
REACT_APP_SUPPORT_PHONE=+256700000000